int main() {
  // Funções matemáticas
  write(sprintf("%.6f", cos(1.0)) + "\n");  // Saída: 0.540302
  write(sprintf("%.6f", exp(1.0)) + "\n");  // Saída: 2.718282
  write(sprintf("%.6f", log(2.0)) + "\n");  // Saída: 0.693147
  write(sprintf("%.6f", sin(1.0)) + "\n");  // Saída: 0.841471
  write(sprintf("%.6f", sqrt(2.0)) + "\n"); // Saída: 1.414214

  return 0;
}
