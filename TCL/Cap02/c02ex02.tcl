# Funções matemáticas

puts [format "%.5f" [expr {cos(1)}]]  ;# Saída: 0.54030...
puts [format "%.5f" [expr {exp(1)}]]  ;# Saída: 2.71828...
puts [format "%.5f" [expr {log(2)}]]  ;# Saída: 0.69314...
puts [format "%.5f" [expr {sin(1)}]]  ;# Saída: 0.84147...
puts [format "%.5f" [expr {sqrt(2)}]] ;# Saída: 1.41421...
